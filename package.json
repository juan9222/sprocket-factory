{
   "name": "sprocket-factory",
   "version": "1.0.0",
   "description": "Awesome project developed with Clean Architecture",
   "scripts": {
      "start": "node ./dist/index.js",
      "test": "jest",
      "seed": "ts-node -r tsconfig-paths/register src/deployment/seed.ts",
      "build": "rimraf dist && tsc -p tsconfig-build.json",
      "watch": "nodemon --exec \"npm run build && npm run start\" --watch src --ext ts"
   },
   "dependencies": {
      "@tsclean/core": "^1.10.13",
      "dotenv": "^16.3.1",
      "helmet": "^7.0.0",
      "module-alias": "^2.2.3",
      "mongoose": "^8.4.1",
      "tsconfig-paths": "^4.2.0"
   },
   "devDependencies": {
      "@types/jest": "^29.5.12",
      "@types/mongoose": "^5.11.97",
      "@types/node": "^20.8.10",
      "jest": "^29.7.0",
      "nodemon": "^3.0.1",
      "rimraf": "^5.0.5",
      "ts-jest": "^29.1.4",
      "ts-node": "^10.9.1",
      "typescript": "^5.2.2"
   },
   "_moduleAliases": {
      "@": "dist"
   },
   "engines": {
      "node": ">=19.9.0"
   }
}
